alias cd..="cd .."
alias ee="exit"
alias jctl='journalctl'
alias l='exa -1F'
alias la='l -a'
alias ll='exa -lF'
alias lla='exa -lFa'
alias ls-l='ls -lF'
alias ls='ls --color=auto -F'
alias nctl='netctl'
alias nctla-ls='netctl-auto list'
alias nctla-sw='netctl-auto switch-to'
alias nctla='netctl-auto'
alias p='pacman'
alias psgrep='ps -ef | grep -v grep | grep -i '
alias sctl='systemctl'
alias sscp='SSH_AUTH_SOCK= scp'
alias sssh='SSH_AUTH_SOCK= ssh'
