alias g='git'
alias gaa='git aa'
alias gac='gaa && gci'
alias gbb='git bb'
alias gci='git ci'
alias gco='git co'
alias gdiff='git diff'
alias glance='glances --disable-check-update --diskio-iops --fs-free-space'
alias gll='git ll'
alias gp='gopass'
alias gph='git push'
alias gpl='git pull'
alias gpp='gpl && gph && gpt'
alias gpt='git push --tags'
alias grep='grep --color=auto'
alias gss='git ss'
alias gtt='git tag'
